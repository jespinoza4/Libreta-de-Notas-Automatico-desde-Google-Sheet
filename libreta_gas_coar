function numero3B01(){
  
  var carpetaMaestra = DriveApp.getFolderById("1qD36FxaYax1YzC-7PgjRA0I8UpkwXEUB");
  var hoja = SpreadsheetApp.getActiveSpreadsheet( );
  var blob = hoja.getBlob();
  
  hoja.getRange("A4").activate();
  hoja.getCurrentCell().setValue("3B01");
  if (hoja.getRange('A4').getValue() == '3B01') {
    Utilities.sleep(4000);
    carpetaMaestra.createFile(blob).setName("ESTUDIANTE_COAR_3B01");  
  
    }
  
  }



