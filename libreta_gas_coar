  function Automatico3A() {
  var spreadsheet = SpreadsheetApp.getActive();
  spreadsheet.getRange('A4').activate();
  numero3A01();
  numero3A02();
  numero3A03();
  numero3A04();
  numero3A05();
  numero3A06();
  numero3A07();
  numero3A08();
  numero3A09();
  numero3A10();
  numero3A11();
  numero3A12();
  numero3A13();
